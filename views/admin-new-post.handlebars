<form name="new-post" id="new-post" method="POST" action="/dashboard/create">
  <input type="text" name="title" id="title" placeholder="Title here" />
  <textarea
    placeholder="Write here"
    wrap="off"
    name="body"
    id="body"
    cols="30"
    rows="10"
  ></textarea>
  <button type="submit">Create Post</button>
</form>

<script src="/public/js/newpost.js"></script>